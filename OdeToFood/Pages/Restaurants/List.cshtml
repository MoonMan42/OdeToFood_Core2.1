@page
@model OdeToFood.Pages.Restaurants.ListModel
@{
    ViewData["Title"] = "List";
}

<h2>Restaurants</h2>

<table class="table">
    @foreach (var r in Model.restaurant)
    {
        <tr>
            <td>@r.Name</td>
            <td>@r.Location</td>
            <td>@r.Cuisine</td>
        </tr>
    }
</table>